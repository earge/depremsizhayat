                </div>



            </div>
        </div>
    </div>

    <script>
        $.ajax({
            url: "https://api.orhanaydogdu.com.tr/deprem/live.php?limit=10",
            type: "POST",
            success: function (data) {


                let datas = Array.from(data.result).map(({ lokasyon, mag, date }) => ({ lokasyon, mag, date }))
                let table = document.createElement("table")
                table.border = "1"
                datas.forEach(item => {  
                    let tr = document.createElement("tr")
                    Object.values(item).forEach((items,index) => {
                        let td = document.createElement("td")
                        td.innerHTML = items
                        tr.appendChild(td)
                    })
                    table.appendChild(tr)
                })
                //document.querySelector("#last-earthquakes").appendChild(table)
            }

        })
    </script>


</body>
</html>

